<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <h1>User List</h1>
    <%= link_to "Gamer Tags", gamertags_path %>

    <table class="table table-striped">
      <caption></caption>
      <thead>
        <tr>
          
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Zipcode</th>
          <th>User Profile</th>

        </tr>
      </thead>
      <tbody>
      	<% @users.each do |b| %>
        <tr>
          
          <td><%= link_to b.first_name, user_path(b) %></td>
          <td><%= b.last_name %></td>
          <td><%= b.email %></td>
          <td><%= b.zipcode %></td>
          <td><%= link_to "Profile", user_path(b) %></td>
          

        </tr>
        <% end %>
        
      </tbody>
    </table>

    <ul class="pagination">
      <li class="disabled"><a href="#">&laquo;</a></li>
      <li class="active"><a href="#">1 <span class="sr-only">(current)</span></a></li>
    </ul>

  </div>
  <div class="col-md-1"></div>
</div>