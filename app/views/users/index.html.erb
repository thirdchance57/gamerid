<h1>User List</h1>

<table class="table">
      <caption>Optional table caption.</caption>
      <thead>
        <tr>
          
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Zipcode</th>
          <th>Edit</th>
          <th>Delete</th>

        </tr>
      </thead>
      <tbody>
      	<% @users.each do |b| %>
        <tr>
          
          <td><%= b.first_name %></td>
          <td><%= b.last_name %></td>
          <td><%= b.email %></td>
          <td><%= b.zipcode %></td>
          <td><%= link_to "Edit", edit_user_path(b) %></td>
          <td><%= link_to "Delete", delete_user_path(b), :method => :delete %></td>

        </tr>
        <% end %>
        
      </tbody>
    </table>